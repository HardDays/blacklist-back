{
  "apiVersion": "1.0",
  "swaggerVersion": "1.2",
  "basePath": "https://blacklist-back.herokuapp.com",
  "resourcePath": "authenticate",
  "apis": [
    {
      "path": "/auth/login.json",
      "operations": [
        {
          "summary": "Authorize by email and password",
          "parameters": [
            {
              "paramType": "form",
              "name": "email",
              "type": "string",
              "description": "Email",
              "required": true
            },
            {
              "paramType": "form",
              "name": "password",
              "type": "password",
              "description": "Password",
              "required": true
            }
          ],
          "responseMessages": [
            {
              "code": 403,
              "responseModel": null,
              "message": "Forbidden"
            }
          ],
          "nickname": "Authenticate#login",
          "method": "post"
        }
      ]
    },
    {
      "path": "/auth/logout.json",
      "operations": [
        {
          "summary": "Logout",
          "parameters": [
            {
              "paramType": "header",
              "name": "Authorization",
              "type": "string",
              "description": "Authentication token",
              "required": true
            }
          ],
          "responseMessages": [
            {
              "code": 400,
              "responseModel": null,
              "message": "Bad Request"
            }
          ],
          "nickname": "Authenticate#logout",
          "method": "post"
        }
      ]
    }
  ],
  "authorizations": null
}